   "version": "0.1.0",
   "description": "Un'applicazione a supporto delle attivit√† svolte da UTEA",
   "license": "MIT",
   "readmeFilename": "README.md",
   "author": {
      "name": "Alessandro Varesi",
      "email": "a.varesi@arol.it"
   },
   "repository": {
      "type": "git",
      "url": "https://github.com/arol-varesi/utea-app"
   },
   "bugs": {
      "url": "https://github.com/arol-varesi/utea-app/issues"
   },
   "tags": [
      "electron",
      "nodejs",
      "typescript",
      "typescript-orm",
      "orm",
      "vue",
      "quasar"
   ],
   "dependencies": {
      "@quasar/extras": "^1.1.1",
      "@types/node": "^11.12.2",
      "bootstrap": "^4.3.1",
      "csv-parser": "^2.2.0",
      "datatables.net": "^1.10.19",
      "datatables.net-colreorder-dt": "^1.5.1",
      "datatables.net-dt": "^1.10.19",
      "jquery": "^3.3.1",
      "material-design-icons-iconfont": "^4.0.5",
      "popper.js": "^1.14.7",
      "quasar": "^1.0.0-rc.4",
      "source-map-support": "^0.5.12",
      "sqlite": "^3.0.3",
      "sqlite3": "^4.0.6",
      "typeorm": "^0.2.16",
      "vue": "^2.6.10",
      "vue-router": "^3.0.6",
      "vuetify": "^1.5.8"
   },
   "devDependencies": {
      "electron": "^5.0.0",
      "electron-builder": "^20.8.1",
      "electron-rebuild": "^1.8.4",
      "electron-webpack": "^2.6.2",
      "electron-webpack-eslint": "^4.0.1",
      "electron-webpack-vue": "^2.2.3",
      "eslint": "^5.16.0",
      "eslint-loader": "^2.1.2",
      "eslint-plugin-html": "^5.0.3",
      "eslint-plugin-vue": "^5.2.2",
      "typeorm-model-generator": "^0.3.2",
      "typescript": "^3.4.1",
      "vue-devtools": "^5.0.0-beta.1",
      "webpack": "^4.30.0"
   },
   "scripts": {
      "dev": "electron-webpack dev",
      "compile": "electron-webpack",
      "dist": "yarn compile && electron-builder",
      "dist:dir": "yarn dist -- --dir -c.compression=store -c.mac.identity=null",
      "compile:db": "tsc --build models/tsconfig.json",
      "try:electron": "tsc --build models/tsconfig.json && electron try/electron/main/main.js",
      "try:spa": "tsc --build models/tsconfig.json && electron try/electron/main/router.js",
      "try:csv": "electron try/csv/csvMain.js",
      "try:orm": "electron try/orm/ormMain.js",
      "try:vue": "electron try/vue/vueMain.js",
      "postinstall": "electron-builder install-app-deps",
      "rebuild": "electron-rebuild -f -w sqlite3",
      "pack": "electron-builder --dir"
   },
   "electronWebpack": {
      "commonSourceDirectory": "src/common",
      "title": "UTEA App"
   }
}
